<h1>User Account Details</h1>

<table>

  <tr>
    <th>Account Number</th>
    <th>Status</th>
    <th>Balance</th>
  </tr>

  <% @accounts.each do |account| %>
    <tr>
      <td><%= account.account_number %></td>
      <td><%= account.status %></td>
      <td><%= account.balance %></td>
    </tr>
  <% end %>
</table>

<%= link_to 'Back', users_path%>